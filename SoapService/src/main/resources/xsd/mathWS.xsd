<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
           xmlns:ns="http://math.ws.divanov" targetNamespace="http://math.ws.divanov">

    <xs:element name="solveQuadraticEducationRequest" type="ns:SolveQuadraticEducationRequest"/>
    <xs:element name="solveQuadraticEducationResponse" type="ns:SolveQuadraticEducationResponse"/>
    <xs:element name="solveQuadraticEducationException" type="ns:FaultDetail"/>

    <xs:complexType name="SolveQuadraticEducationRequest">
        <xs:sequence>
            <xs:element name="a" minOccurs="1" maxOccurs="1" type="xs:double"/>
            <xs:element name="b" minOccurs="1" maxOccurs="1" type="xs:double"/>
            <xs:element name="c" minOccurs="1" maxOccurs="1" type="xs:double"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SolveQuadraticEducationResponse">
        <xs:sequence>
            <xs:element name="formula" minOccurs="1" maxOccurs="1" type="xs:string"/>
            <xs:element name="d" minOccurs="1" maxOccurs="1" type="xs:double"/>
            <xs:element name="x1" minOccurs="1" maxOccurs="1" type="xs:double"/>
            <xs:element name="x2" minOccurs="0" maxOccurs="1" type="xs:double"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="FaultDetail">
        <xs:sequence>
            <xs:element name="formula" minOccurs="1" maxOccurs="1" type="xs:string"/>
            <xs:element name="d" minOccurs="1" maxOccurs="1" type="xs:double"/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>